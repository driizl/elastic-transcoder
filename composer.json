{
    "name": "driizl/elastic-transcoder",
    "description": "AWS Elastic Transcoder Service Integration",
    "keywords": [
        "laravel",
        "aws",
        "elastic",
        "package"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Yusniel Brocard",
            "email": "brocard@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.0",
        "aws/aws-sdk-php": "~3.0"
    },
    "autoload": {
        "psr-4": {
            "Driizl\\Elastic\\": "src/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        },
        "laravel": {
            "providers": [
                "Driizl\\Elastic\\ElasticServiceProvider"
            ],
            "aliases": {
                "Elastic": "Driizl\\Elastic\\ElasticFacade"
            }
        }
    }
}
