{
    "name": "driizl/elastic-transcoder",
    "description": "AWS Elastic Transcoder Service Integration",
    "license": "MIT",
    "authors": [
        {
            "name": "Yusniel Brocard",
            "email": "brocard@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.0",
        "aws/aws-sdk-php": "~3.0"
    },
    "autoload": {
        "psr-4": {
            "Driizl\\Elastic\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "minimum-stability": "dev",
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        },
        "laravel": {
            "providers": [
                "Driizl\\Elastic\\ServiceProvider"
            ],
            "aliases": {
                "Elastic": "Driizl\\Elastic\\Facade"
            }
        }
    }
}
